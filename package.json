{
  "name": "simonyi-web-next-13",
  "version": "3.0.0",
  "private": true,
  "description": "simonyi-web-next-13",
  "author": "triszt4n, gergoradeczki",
  "keywords": [
    "next",
    "react",
    "contentful",
    "typescript",
    "tailwind",
    "pnpm"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "eslint": "eslint --max-warnings=0",
    "eslint:fix": "eslint --max-warnings=0 --fix .",
    "contentful:typegen-offline": "cf-content-types-generator src/contentful/export.json --v10 --out src/@types/generated",
    "contentful:typegen": "dotenv -e .env -- cross-var cf-content-types-generator --v10 -s %CONTENTFUL_SPACE_ID% -t %CONTENTFUL_MANAGEMENT_API_ACCESS_TOKEN% -o src/@types/generated"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "16.1.0",
    "@contentful/rich-text-types": "17.1.0",
    "@tailwindcss/postcss": "4.1.11",
    "clsx": "2.1.1",
    "contentful": "11.7.15",
    "contentful-management": "11.54.4",
    "gray-matter": "4.0.3",
    "next": "15.4.5",
    "next-intl": "4.3.4",
    "postcss": "8.5.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.11",
    "yet-another-react-lightbox": "3.25.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@types/node": "24.1.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "autoprefixer": "10.4.21",
    "cf-content-types-generator": "2.16.0",
    "cross-var": "1.1.0",
    "dotenv": "17.2.1",
    "dotenv-cli": "10.0.0",
    "eslint": "9.32.0",
    "eslint-config-next": "15.4.5",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "util": "0.12.5"
  },
  "engines": {
    "node": "^22",
    "pnpm": "^9 || ^10"
  }
