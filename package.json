    "@mdx-js/loader": "2.3.0",
    "@mdx-js/react": "2.3.0",
    "@next/mdx": "13.4.19",
    "bright": "0.8.4",
    "contentful": "10.4.2",
    "contentful-management": "10.39.2",
    "gray-matter": "4.0.3",
    "next": "13.4.19",
    "next-mdx-remote": "4.4.1",
